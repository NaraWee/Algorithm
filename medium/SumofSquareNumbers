class Solution {
    public boolean judgeSquareSum(int c) {
        
        for(int a = 0; a*a <= c/2; a++) {
            
            double sqrt = Math.sqrt(c - a*a);
            int b = (int) sqrt;
            
            if(b == sqrt) {
                return true;
            }
            
        }
        
        return false;
        
    }
}
